<configuration debug="true">
	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{yyyy-MM-dd'T'HH:mm:ss.SSS} %-5level \(%file:%line\) [%thread] :: %msg%n</pattern>
		</encoder>
	</appender>

	<logger name="org.carthageking" level="trace" />

	<root level="info">
		<appender-ref ref="STDOUT" />
	</root>
</configuration>